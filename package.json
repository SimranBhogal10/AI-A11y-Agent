    "build": "node esbuild.config.js",
    "watch": "node esbuild.config.js --watch",
    "package": "vsce package"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "node-fetch": "^3.3.2",
    "openai": "^5.1.1"
  },
  "devDependencies": {
    "esbuild": "^0.25.5",
    "vscode": "^1.1.37"
  }

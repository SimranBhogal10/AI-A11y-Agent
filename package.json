{
  "name": "groq-ai-agent",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "type": "module",
  "main": "./out/extension.cjs",
  "activationEvents": ["onCommand:extension.checkAccessibility"],
  "contributes": {
    "commands": [
      {
        "command": "extension.checkAccessibility",
        "title": "Check Accessibility with AI Agent"
      }
    ]
  },
  "scripts": {
